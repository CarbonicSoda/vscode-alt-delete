{
	"name": "alt-delete",
	"displayName": "Alt Delete",
	"description": "Keymaps of Ctrl-Del cousins for your ease",
	"version": "2.1.2",
	"publisher": "carbonsoda",
	"author": {
		"name": "David W."
	},
	"homepage": "https://marketplace.visualstudio.com/items?itemName=carbonsoda.alt-delete",
	"repository": {
		"type": "git",
		"url": "https://github.com/CarbonicSoda/vscode-alt-delete"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/CarbonicSoda/vscode-alt-delete/issues",
		"email": "carbonsoda@outlook.com"
	},
	"engines": {
		"vscode": "^1.80.0"
	},
	"keywords": [
		"keys",
		"alt",
		"ctrl",
		"delete",
		"backspace",
		"left",
		"right",
		"shift"
	],
	"categories": [
		"Keymaps"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"icon": "media/icon.png",
	"main": "./dist/extension",
	"browser": "./dist/web-extension",
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"compile": "webpack",
		"watch": "tsc --watch -p ./",
		"webpack-dev": "webpack --mode development --watch",
		"webpack": "webpack --mode development",
		"package": "webpack --mode production --devtool hidden-source-map",
		"pretest": "npm run compile-tests && npm run compile && npm run lint",
		"lint": "eslint src"
	},
	"devDependencies": {
		"@types/vscode": "^1.80.0",
		"@types/mocha": "^10.0.6",
		"@types/node": "18.x",
		"eslint": "^8.57.0",
		"typescript": "^5.4.5",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.3.9"
	},
	"contributes": {
		"commands": [
			{
				"title": "Alt-Backspace",
				"command": "alt-delete.alt-backspace"
			},
			{
				"title": "Alt-Delete",
				"command": "alt-delete.alt-delete"
			},
			{
				"title": "Alt-Left",
				"command": "alt-delete.alt-left"
			},
			{
				"title": "Alt-Right",
				"command": "alt-delete.alt-right"
			},
			{
				"title": "Alt-Shift-Left",
				"command": "alt-delete.alt-shift-left"
			},
			{
				"title": "Alt-Shift-Right",
				"command": "alt-delete.alt-shift-right"
			}
		],
		"keybindings": [
			{
				"command": "alt-delete.alt-backspace",
				"key": "Alt+Backspace"
			},
			{
				"command": "alt-delete.alt-delete",
				"key": "Alt+Delete"
			},
			{
				"command": "alt-delete.alt-left",
				"key": "Alt+,"
			},
			{
				"command": "alt-delete.alt-left",
				"key": "Alt+-"
			},
			{
				"command": "alt-delete.alt-right",
				"key": "Alt+."
			},
			{
				"command": "alt-delete.alt-right",
				"key": "Alt+="
			},
			{
				"command": "alt-delete.alt-shift-left",
				"key": "Alt+Shift+,"
			},
			{
				"command": "alt-delete.alt-shift-left",
				"key": "Alt+Shift+-"
			},
			{
				"command": "alt-delete.alt-shift-right",
				"key": "Alt+Shift+."
			},
			{
				"command": "alt-delete.alt-shift-right",
				"key": "Alt+Shift+="
			}
		]
	}
}
